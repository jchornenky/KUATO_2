{
    "name": "backend",
    "version": "0.0.1",
    "description": "kuato rest api",
    "main": "server.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jchornenky/KUATO_Backend.git"
    },
    "keywords": [
        "Express",
        "RestAPI",
        "MongoDB",
        "Mongoose"
    ],
    "author": "tuku",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jchornenky/KUATO_Backend/issues"
    },
    "homepage": "https://github.com/jchornenky/KUATO_Backend#readme",
    "eslintConfig": {
        "extends": "airbnb-base",
        "env": {
            "es6": true,
            "browser": true
        },
        "rules": {
            "brace-style": [
                "error",
                "stroustrup"
            ],
            "comma-dangle": [
                "error",
                "never"
            ],
            "no-unused-vars": [
                "warn"
            ],
            "no-var": [
                "off"
            ],
            "one-var": [
                "off"
            ],
            "indent": [
                "error",
                4
            ],
            "max-len": [
                "error",
                {
                    "code": 120
                }
            ]
        }
    },
    "dependencies": {
        "amqplib": "^0.5.5",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "gmail-send": "^1.8.10",
        "googleapis": "^39.2.0",
        "html-pdf": "^2.2.0",
        "moment": "^2.27.0",
        "mongoose": "^5.9.11",
        "node-cron": "^2.0.3",
        "nunjucks": "^3.2.1",
        "password-hash": "^1.2.2",
        "winston": "^3.2.1",
        "winston-loggly-bulk": "^3.1.0",
        "xlsx": "^0.17.0"
    },
    "devDependencies": {
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "eslint": "^7.1.0",
        "eslint-config-airbnb-base": "^14.1.0",
        "eslint-plugin-import": "^2.20.2",
        "express-swagger-generator": "^1.1.17",
        "mocha": "^5.0.1",
        "mocha-silent-reporter": "^1.0.0",
        "mochawesome": "^3.1.2",
        "should": "^13.2.3",
        "supertest": "^4.0.2"
    }
}
